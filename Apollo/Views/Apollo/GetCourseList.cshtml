@model IEnumerable<Apollo.Services.BEANS.CourseBEAN>

@{
    ViewBag.Title = "GetCourseList";
}

<h2>@ViewBag.UniversityName Courses</h2>

<table class="table">
    <tr>
        <th>
           @Html.Label("Name")
        </th>
        <th>
            @Html.Label("Description")
        </th>
        <th>
            @Html.Label("Entry Requirements")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseBEANName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseBEANDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseBEANEntryRequirements)
        </td>
        <td>

        </td>
    </tr>
}

</table>
